package br.com.matheusmenezesdacosta.main;

public class RadioRelogio {

	private int hora;
	private int minuto;
	
	public RadioRelogio(int hora, int minuto) {
		this.hora = hora;
		this.minuto = minuto;
	}
	
	public int getHora() {
		return hora;
	}
	public void setHora(int hora) {
		this.hora = hora;
	}
	public int getMinuto() {
		return minuto;
	}
	public void setMinuto(int minuto) {
		this.minuto = minuto;
	}
}
